<div class="container">
  <div class="panel panel-primary">
    <div class="panel-heading"> <h4>User List</h4></div>
      <div class="panel-body">

        <p id="notice"><%= notice %></p>

        <table>
          <thead>
            <tr>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
            <% @user_registrations.each do |user_registration| %>
              <tr>
               
                <td><%= link_to 'Show', user_registration %></td>
                <td><%= link_to 'Edit', edit_user_registration_path(user_registration) %></td>
                <td><%= link_to 'Destroy', user_registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
          </tbody>
        </table>

        <br>

        <%= link_to 'New User Registration', new_user_registration_path ,class: "btn btn-success"%>
      </div>
    </div>
  </div>
  
